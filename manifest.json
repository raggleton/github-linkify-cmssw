{
    "name": "Github linkify for CMSSW",
    "description": "Turn C++ and Python includes in CMSSW into links on Github",
    "version": "0.3.3",
    "content_scripts": [
        {
        "matches": [
            "https://www.github.com/*/cmssw/*",
            "https://github.com/*/cmssw/*"
            ],
        "js": ["contentscript.js"],
        "run_at": "document_end"
        }
    ],
    "manifest_version": 2,
    "icons": {
      "128": "images/icon/icon_128.png"
    }
}