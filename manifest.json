{
    "name": "Github linkify for CMSSW",
    "description": "Makes browsing CMSSW files waaaay easier.",
    "version": "0.3.2",
    "content_scripts": [
        {
        "matches": [
            "https://www.github.com/*/cmssw/*",
            "https://github.com/*/cmssw/*"
            ],
        "js": ["contentscript.js"],
        "run_at": "document_end"         // pay attention to this line
        }
    ],
    "manifest_version": 2
}